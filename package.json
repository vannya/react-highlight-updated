{
  "name": "react-highlight-updated",
  "version": "0.1.7",
  "private": false,
  "homepage": "https://github.com/vannya/react-highlight-updated/#readme",
  "bugs": {
    "url": "https://github.com/vannya/react-highlight-updated/issues"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/vannya/react-highlight-updated.git"
  },
  "peerDependencies": {
    "react": "^16.8.6",
    "react-dom": "^16.8.6"
  },
  "devDependencies": {
    "@babel/cli": "^7.6.4",
    "@babel/core": "^7.6.4",
    "babel-eslint": "^10.0.3",
    "babel-jest": "^24.9.0",
    "babel-preset-react-app": "^9.0.2",
    "cross-env": "^6.0.3",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.15.1",
    "eslint": "^6.5.1",
    "eslint-config-react-app": "^5.0.2",
    "eslint-plugin-flowtype": "^4.3.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.16.0",
    "eslint-plugin-react-hooks": "^2.1.2",
    "fs-extra": "^8.1.0",
    "highlight.js": "^9.15.10",
    "jest": "^24.9.0",
    "jest-environment-jsdom-fourteen": "^0.1.0",
    "jest-resolve": "^24.9.0",
    "jest-watch-typeahead": "^0.4.0",
    "npm-run-all": "^4.1.5",
    "react": "^16.10.2",
    "react-app-polyfill": "^1.0.4",
    "react-dev-utils": "^9.1.0",
    "react-dom": "^16.10.2",
    "react-test-renderer": "^16.10.2",
    "resolve": "^1.12.0",
    "semver": "^6.3.0"
  },
  "scripts": {
    "build:code": "cross-env NODE_ENV=production babel ./src/lib --out-dir ./dist",
    "build:copy-files": "node scripts/copyDistFiles.js",
    "build": "npm-run-all --parallel build:code build:copy-files",
    "test": "node scripts/test.js --coverage --coverageDirectory=coverage --watchAll=false"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "files": [
    "dist"
  ],
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "collectCoverageFrom": [
      "src/lib/**/*.{js,jsx,ts,tsx}",
      "!src/**/*.d.ts"
    ],
    "setupFiles": [
      "react-app-polyfill/jsdom"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/setupTests.js"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,ts,tsx}",
      "<rootDir>/src/**/*.{spec,test}.{js,jsx,ts,tsx}"
    ],
    "testEnvironment": "jest-environment-jsdom-fourteen",
    "transform": {
      "^.+\\.(js|jsx|ts|tsx)$": "<rootDir>/node_modules/babel-jest"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|ts|tsx)$"
    ],
    "modulePaths": [],
    "moduleFileExtensions": [
      "web.js",
      "js",
      "web.ts",
      "ts",
      "web.tsx",
      "tsx",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ],
    "watchPlugins": [
      "jest-watch-typeahead/filename",
      "jest-watch-typeahead/testname"
    ]
  },
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "modules": "commonjs"
        }
      ],
      [
        "react-app",
        {
          "helpers": false,
          "absoluteRuntime": false
        }
      ]
    ]
  },
  "dependencies": {},
  "keywords": [
    "react",
    "highlight.js",
    "syntax",
    "highlighting",
    "react-component"
  ]
}
